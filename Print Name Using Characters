;Program to Print name using characters.
.model small
.stack 100h
.data
.code
main proc
    
    mov dl,'a'  ;Storing character in data register
    mov ah,2    
    int 21h
    
    mov dl,'l'
    mov ah,2
    int 21h
    
    mov dl,'i'
    mov ah,2
    int 21h
    
    mov ah,4ch
    int 21h
    
    main endp
